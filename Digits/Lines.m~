function lines = Lines(X, y)

lines = 0;

for ipicture = 4:6
%for ipicture = 1:rows(X)
    picture = reshape(X(ipicture,:), 28, []);
    for irow = 1:rows(picture)
        alreadyfound=false;
    	for icolumn = 1:columns(picture)
            line=false;
	    if(X(irow, icolumn)>0 && line) 
	end
    end
%    plot([1,28], [1,28], 'rx', 'MarkerSize', 10);
    y(ipicture)
    picture
end
end
